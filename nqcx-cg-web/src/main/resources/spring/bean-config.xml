<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/util  http://www.springframework.org/schema/util/spring-util-3.2.xsd">

	<util:map id="_configValues">
		<entry key="mainName" value="Code generate" />
	</util:map>

	<bean id="resourceConfig" class="org.nqcx.commons.web.resource.ResourceConfig">
		<property name="configValues">
			<ref bean="_configValues" />
		</property>
	</bean>
	
	<bean id="resultConfig" class="org.nqcx.commons.web.result.ResultConfig">
		<property name="results">
			<map>
				<entry key="E00000"><ref bean="E00000" /></entry>
				<entry key="E00001"><ref bean="E00001" /></entry>
				<entry key="M00000"><ref bean="M00000" /></entry>
				<entry key="S00000"><ref bean="S00000" /></entry>
			</map>
		</property>
	</bean>
	<bean id="E00000" class="org.nqcx.commons.web.result.Result">
		<property name="type" value="error" />
		<property name="code" value="0" />
		<property name="title" value="RE.00000.TITLE" />
		<property name="subject" value="RE.00000.SUBJECT" />
		<property name="auto" value="RE.00000.AUTO" />
	</bean>
	<bean id="E00001" class="org.nqcx.commons.web.result.Result">
		<property name="type" value="error" />
		<property name="code" value="1" />
		<property name="subject" value="RE.00001.SUBJECT" />
		<property name="suggestTitle" value="RE.00000.SUGGESTTITLE" />
		<property name="suggest">
			<list>
				<value>RE.00000.SUGGEST.0001</value>
				<value>RE.00000.SUGGEST.0002</value>
				<value>RE.00000.SUGGEST.0003</value>
			</list>
		</property>
	</bean>

	<bean id="M00000" class="org.nqcx.commons.web.result.Result">
		<property name="type" value="msg" />
		<property name="code" value="0" />
		<property name="title" value="RM.00000.TITLE" />
		<property name="subject" value="RM.00000.SUBJECT" />
		<property name="auto" value="RM.00000.AUTO" />
	</bean>

	<bean id="S00000" class="org.nqcx.commons.web.result.Result">
		<property name="type" value="success" />
		<property name="code" value="0" />
		<property name="title" value="RS.00000.TITLE" />
		<property name="subject" value="RS.00000.SUBJECT" />
		<property name="auto" value="RS.00000.AUTO" />
	</bean>
</beans>